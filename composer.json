{
  "scripts": {
    "framework-tests": "./vendor/bin/phpunit -c ./phpunit.xml --testsuite framework",
    "application-tests": "./vendor/bin/phpunit -c ./phpunit.xml --testsuite application"
  },
  "require": {
    "psr/container": "2.0.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Framework\\": "framework/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.20"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}